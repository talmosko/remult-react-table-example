{
  "name": "react-vite-express-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"ts-node-dev -P tsconfig.server.json src/server/\" \"vite\" -k -n \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\"",
    "dev-node": "tsx watch src/server",
    "start": "node dist/server/",
    "build": "tsc && vite build && tsc -p tsconfig.server.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.9.2",
    "concurrently": "^8.2.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remult": "^0.20.0",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.12.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
